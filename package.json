{
  "name": "@your-org/swagger-mcp",
  "version": "0.1.0",
  "description": "MCP server for accessing Swagger/OpenAPI documentation",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "swagger-mcp": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "lint": "biome check --write .",
    "lint:check": "biome check .",
    "format": "biome format --write .",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "keywords": ["mcp", "swagger", "openapi"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.10.2",
    "@vitest/coverage-v8": "^2.1.8",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
